{
	"parser": "babel-eslint",
	"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
	"env": {
        "browser": true
    },
	"extends": "eslint:recommended",
	"rules": {
		"indent": ["error", "tab"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"no-console": "off",
		"react/jsx-uses-vars": 2
	},
	"plugins": [
    	"react"
  	]
}
